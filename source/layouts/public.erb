<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">

  <title>Queen City Merge</title>
  <meta name="description" content="">
  <meta name="author" content="Made in Cincy">
  <meta name="viewport" content="width=device-width,initial-scale=1">

  <link href="stylesheets/screen.css" media="screen" rel="stylesheet" type="text/css" />
  <%= javascript_include_tag "http://cdnjs.cloudflare.com/ajax/libs/modernizr/2.6.2/modernizr.min.js" %>
  <%= javascript_include_tag "public" %>
  
  <script type="text/javascript" src="//use.typekit.net/gyn0xoh.js"></script>
  <script type="text/javascript">try{Typekit.load();}catch(e){}</script>

</head>
<body data-spy="scroll" data-target=".navbar" class="<%= page_classes %>">

  
  <header class="site-header">
    <section class="header-content">
      <hgroup>
        <h1>QC Merge</h1>
        <h2>Let us go forward <span>Together</span></h2>
      </hgroup>
      <div class="loc-time-date">
        <span class="building">
          Contemporary Arts Center
        </span>
        <span class="date">
          May 10 &amp; 11 2013
        </span>
        <span class="loc">
          Cincinnati Ohio
        </span>
      </div>
      <!-- /loc-time-date -->
    </section>
    <!-- /header-content -->
    
  </header>
  <!-- /site-header -->

  <nav id="nav" class="navbar">
    <div class="navbar-inner">
      <!-- .btn-navbar is used as the toggle for collapsed navbar content -->
      <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
        <span class="icon-bar"></span>
      </a>
       

      <a class="brand" href="#">QC Merge</a>
      <div class="nav-collapse collapse">
        <ul class="nav">
          <li><a href="#about">About</a></li>
          <li><a href="#venue">Venue</a></li>
          <li><a href="#sponsors">Sponsors</a></li>
        </ul>
        <!-- /nav -->
      </div>
      <!-- /nav-collapse -->
    </div>
    <!-- /navbar-inner -->
  </nav>
  <!-- /navbar -->

  <div id="main-content" class="main-content">
    <%= yield %>
  </div>
  <!-- /main-content -->

  <footer class="site-footer">

    <section class="footer-content">
      <div class="more-qcmerge">
        <h3>Want More QCMerge?</h3>
        <p>Check out some of last year's talks on <%= link_to 'YouTube', "https://www.youtube.com/playlist?list=PL2CDtLMcqrFldU4dLHoNv0T2SPetq8WXo", target: '_blank' %></p>
        <hr />
        <%= link_to 'Want to be a sponsor? Find out more info here.', 'QCMerge_Sponsor.pdf', :class => 'sponsor' %>

      </div>
      <!-- /more-qcmerge -->
      <div class="etc">
        <div class="social">
          <h3>Get to know us better</h3>
          <%= link_to '<i class="icon-twitter"></i>', 'http://twitter.com/qcmerge', class: 'twitter' %>
          <%= link_to '<i class="icon-facebook"></i>', 'http://facebook.com/qcmerge', class: 'facebook' %>

        </div>
        <hr />
        <div class="last-year">
          <h3>Looking for 2012's site?</h3>
          <p>It can be found <%= link_to 'here', '/2012' %>.</p>
        </div>
      </div>
      <!-- /etc -->
      <div class="help-copy">
        <div class="help-out">
          <h3>Want to know more?</h3>
          <form class="help-form">
            <div class="input-append">
              <label>Drop us a line and we'll be in touch</label>
              <input class="span2" id="" type="email" placeholder="name@domain.com" />
              <button class="btn" type="button">OK</button>
            </div>
          </form>
        </div>
        <!-- /help-out -->

        <hr />
        <div class="copyright">
          &copy; Copyright 2013 QCMerge
        </div>
        <!-- /copyright -->
      </div>
      <!-- /help-out -->
    </section>
    <!-- /footer-content -->

  </footer>
  <!-- /site-footer -->


  <script>
    var _gaq=[['_setAccount','UA-30041998-1'],['_trackPageview'],['_trackPageLoadTime']];
    (function(d,t){var g=d.createElement(t),s=d.getElementsByTagName(t)[0];
    g.src=('https:'==location.protocol?'//ssl':'//www')+'.google-analytics.com/ga.js';
    s.parentNode.insertBefore(g,s)}(document,'script'));
  </script>

  <!--[if lt IE 7 ]>
    <script defer src="//ajax.googleapis.com/ajax/libs/chrome-frame/1.0.3/CFInstall.min.js"></script>
    <script defer>window.attachEvent('onload',function(){CFInstall.check({mode:'overlay'})})</script>
  <![endif]-->

</body>
</html>
